.section__title-wrapper {
  position: relative;
}

.section__title {
  font-family: "Open Sans";
  color: $TextColor;
  font-size: 20px;
  font-weight: 700;
  text-transform: uppercase;
  margin-bottom: 40px;
}

.section__title-after {
  position: absolute;
  top: 34px;
  display: block;
  width: 70px;
  height: 6px;
}
