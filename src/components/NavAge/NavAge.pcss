.nav-age {
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: flex-start;
}

.nav-age__icon {
  &:before {
    content: '';
    display: flex;
    background: svg-load('Age.svg', fill=$NavColor, width=100%, height=100%) center no-repeat;
    width: 34px;
    height: 17px;
    margin-right: 30px;
  }
}